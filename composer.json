{
    "name": "digitools/laravel-multi-tenancy",
    "description": "A Laravel Multi-Tenancy Toolkit for SaaS applications",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Digitools\\MultiTenancy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitools\\MultiTenancy\\Providers\\MultiTenancyServiceProvider"
            ]
        }
    }
}
